{
    "name": "ffmpeg",
    "no-autogen": true,
    "config-opts": [
        "--disable-x86asm",
        "--enable-shared",
        "--enable-version3",
        "--disable-large-tests",
        "--enable-linux-perf",
        "--disable-debug",
        "--disable-programs",
        "--quiet"
        ],
    "sources": [
    {
        "type": "git",
        "url": "https://github.com/FFmpeg/FFmpeg.git",
        "tag": "release/5.0"
    }
    ],
    "cleanup": [
        "/share/ffmpeg"
    ]
}
